---- Create a Book List

CREATE TABLE book (
    id INTEGER PRIMARY KEY, 
    name TEXT, 
    rating INTEGER
);


---- Write 3 favorite books 

INSERT INTO book VALUES (1, "Harry Potter", 4);
INSERT INTO book VALUES (2, "Games of Thrones", 1);
INSERT INTO book VALUES (3, "Camaleon", 1);



---- Display Book List

SELECT * FROM book;

